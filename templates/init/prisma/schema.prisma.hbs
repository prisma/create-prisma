generator client {
  provider = "prisma-client"
  output   = "./generated"
}

datasource db {
  provider = "{{provider}}"
}
{{#if (eq schemaPreset "basic")}}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
{{/if}}
